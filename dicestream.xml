                    </div>
                    <div id="diceDiv" ng-include="'https://dl.dropbox.com/u/1177409/dicestream/partials/rolledDiceInterface.html'"></div>
                </div>
            </tab>
            <tab heading="Text">TextTab</tab>
            <tab heading="Lower Third">Lower Third</tab>
            <tab heading="Settings">Settings</tab>
        </tabset>
    </div>


